    print("  I      Rho         U          V          P          T          M        Pt")
    for i in range ( self.neta, 0, -1 ):
        t = self.hinf - 0.5*(fs.u[i]**2 + fs.v[i]**2)
        pt2 = fs.p[i]
        ptg = "----------"
        xm = 0.0
        if(i != 1):
            ptg = "|          ";
            xm = math.sqrt((fs.u[i]**2+fs.v[i]**2)/(0.4*t))
            pt2 = fs.p[i]/fs.pinf*(xm/fs.xminf)**7 \
                *((7.0*fs.xminf**2-1.0)/(7.0*xm**2-1.0))**2.5
        j = int(pt2/3.0 *10.0) +1
